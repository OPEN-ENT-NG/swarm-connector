{
    "name": "fr.openent~swarm-connector~{{ swarmConnectorVersion }}",
    {% if (item == 'jobs') %}
    "worker" : true,
    {% endif %}
    "config": {
        "main" : "fr.openent.swarm.Swarm",
        "port" : 8384,
        "app-name" : "Swarm-connector",
        "app-address" : "/swarm",
        "app-icon" : "Swarm-logo-large",
        "host": "https://{{ host }}",
        "ssl" : true,
        "auto-redeploy": false,
        "integration-mode" : "HTTP",
        "app-registry.port" : 8012,
    }
}